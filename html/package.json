{
  "name": "latanda-frontend",
  "version": "2.0.0",
  "description": "La Tanda Fintech Platform - Frontend Application",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "vite build",
    "build:prod": "NODE_ENV=production vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview --host 0.0.0.0",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules",
    "reinstall": "npm run clean:all && npm install",
    "audit": "npm audit --production",
    "audit:fix": "npm audit fix",
    "update:check": "npm outdated",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch"
  },
  "keywords": [
    "fintech",
    "tanda",
    "web3",
    "cryptocurrency",
    "defi",
    "blockchain",
    "honduras"
  ],
  "author": "La Tanda Team <contact@latanda.online>",
  "license": "PROPRIETARY",
  "repository": {
    "type": "git",
    "url": "https://github.com/latanda/latanda-frontend"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@lhci/cli": "^0.15.1",
    "@vitejs/plugin-legacy": "^7.2.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.2.0",
    "lighthouse": "^13.0.1",
    "rollup-plugin-visualizer": "6.0.5",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.1.0",
    "workbox-window": "^7.4.0"
  },
  "dependencies": {
    "web-vitals": "^5.1.0"
  },
  "overrides": {
    "tmp": ">=0.2.4"
  }
}
